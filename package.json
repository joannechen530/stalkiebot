{
  "name": "stalkiebot",
  "version": "1.0.0",
  "description": "nwHacks 2019 project",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./typings/; rm -rf ./node_modules/;",
    "cleanwin": "rmdir typings /s /q & rmdir node_modules /s /q & exit 0",
    "configure": "npm install; typings install --global;",
    "configurewin": "npm install & typings install --global",
    "build": "tsc",
    "test": "mocha -c -R mochawesome --timeout 10000",
    "cover": "istanbul cover _mocha -- --timeout 10000",
    "coverwin": "istanbul cover node_modules/mocha/bin/_mocha -- --timeout 10000",
    "start": "node src/App.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joannechen530/stalkiebot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joannechen530/stalkiebot/issues"
  },
  "homepage": "https://github.com/joannechen530/stalkiebot#readme",
  "dependencies": {
    "jszip": "^3.1.2",
    "mochawesome": "^1.5.2",
    "parse5": "^2.2.3",
    "restify": "^4.0.3",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "icedfrisby": "^0.2.4",
    "istanbul": "^0.4.4",
    "joi": "^8.4.2",
    "mocha": "^2.5.3",
    "typescript": "^1.8.7",
    "typings": "^1.3.0"
  }
}
